{
    "extends": "airbnb-base",
    "rules": {
        "no-console": "off",
        "no-constant-condition": "off",
        "implicit-arrow-linebreak": "off",
        "no-plusplus": "off",
        "arrow-parens": "off",
        "no-restricted-syntax": "off",
        "no-restricted-properties": "off"
    }
}
